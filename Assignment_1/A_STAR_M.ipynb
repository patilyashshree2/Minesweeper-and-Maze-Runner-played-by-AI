    "import random\n",
    "def genMaze(dim,p):\n",
    "    maze = []\n",
    "    for x in range(dim):\n",
    "        row = []\n",
    "        for y in range(dim):\n",
    "            if random.random() > p:\n",
    "                row.append(0)\n",
    "            else :\n",
    "                row.append(1)\n",
    "        maze.append(row)\n",
    "    maze[0][0] = 0\n",
    "    maze[dim-1][dim-1] = 0\n",
    "    return maze"
   ]
